<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kz.pvp.quadraricassistant"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="kz.pvp.quadraricassistant.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="kz.pvp.quadraricassistant.ChoiceMenu"
            android:label="@string/title_activity_choice_menu" >
        </activity>
        <activity
            android:name="kz.pvp.quadraricassistant.CalculationListActivity"
            android:label="@string/title_activity_calculation_list" >
        </activity>
        <activity
            android:name="kz.pvp.quadraricassistant.CalculationsActivity"
            android:label="@string/title_activity_calculations" >
        </activity>
        <activity
            android:name="kz.pvp.quadraricassistant.Calculations"
            android:label="@string/title_activity_calculations" >
        </activity>
    </application>

</manifest>
